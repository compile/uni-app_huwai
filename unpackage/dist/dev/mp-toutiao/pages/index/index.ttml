<view class="container"><view class="mp-search-box"><input class="ser-input" type="text" value="输入关键字搜索" disabled="{{true}}" data-event-opts="{{[['tap',[['search',['$0'],['item']]]]]}}" bindtap="__e"/></view><view class="carousel-section"><view class="titleNview-placing"></view><view class="titleNview-background" style="{{'background-color:'+(titleNViewBackground)+';'}}"></view><swiper class="carousel" circular="{{true}}" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block tt:for="{{carouselList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><swiper-item data-event-opts="{{[['tap',[['navToPage',['$0'],[[['carouselList','',index]]]]]]]}}" class="carousel-item" bindtap="__e"><image src="{{item.pic}}"></image></swiper-item></block></swiper><view class="swiper-dots"><text class="num">{{swiperCurrent+1}}</text><text class="sign">/</text><text class="num">{{swiperLength}}</text></view></view><view class="cate-section"><block tt:for="{{navList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['navToPage',['$0'],[[['navList','',index]]]]]]]}}" class="cate-item" bindtap="__e"><image src="{{item.thumb}}"></image><text>{{item.name}}</text></view></block></view><view class="ad-1"><image src="/static/temp/ad1_2.jpg" mode="scaleToFill"></image></view><view class="seckill-section m-t"><view class="s-header"><image class="s-img" src="/static/temp/secskill-img.jpg" mode="widthFix"></image><text class="tip">8点场</text><text class="hour timer">{{time}}</text><text class="minute timer">{{minute}}</text><text class="second timer">{{second}}</text><text class="yticon icon-you"></text></view><scroll-view class="floor-list" scroll-x="{{true}}"><view class="scoll-wrapper"><block tt:for="{{goodsList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['goodsList','',index]]]]]]]}}" class="floor-item" bindtap="__e"><image src="{{item.thumb}}" mode="aspectFill"></image><text class="title clamp">{{item.title}}</text><text class="price">{{"￥"+item.price}}</text></view></block></view></scroll-view></view><view class="f-header m-t" style="display:none;"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">精品推荐</text><text class="tit2">Boutique Group Buying</text></view><text class="yticon icon-you"></text></view><view class="group-section" style="display:none;"><swiper class="g-swiper" duration="{{500}}"><block tt:for="{{goodsList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><block tt:if="{{index%2===0}}"><swiper-item data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['goodsList','',index]]]]]]]}}" class="g-swiper-item" bindtap="__e"><view class="g-item left"><image src="{{item.thumb}}" mode="aspectFill"></image><view class="t-box"><text class="title clamp">{{item.title}}</text><view class="price-box"><text class="price">{{"￥"+item.price}}</text><text class="m-price">￥188</text></view><view class="pro-box"><view class="progress-box"><progress percent="72" activeColor="#fa436a" active="{{true}}" stroke-width="6"></progress></view><text>6人成团</text></view></view></view></swiper-item></block></block></swiper></view><view class="f-header m-t"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">精选</text><text class="tit2">Competitive Products For You</text></view><text class="yticon icon-you"></text></view><view class="hot-floor"><view class="floor-img-box"><image class="floor-img" src="https://img1.baidu.com/it/u=3046496105,2138098779&fm=253&fmt=auto&app=120&f=JPEG?w=708&h=485" mode="scaleToFill"></image></view><scroll-view class="floor-list" scroll-x="{{true}}"><view class="scoll-wrapper"><block tt:for="{{goodsList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['goodsList','',index]]]]]]]}}" class="floor-item" bindtap="__e"><image src="{{item.thumb}}" mode="aspectFill"></image><text class="title clamp">{{item.title}}</text><text class="price">{{"￥"+item.price}}</text></view></block><view class="more"><text>查看全部</text><text>More+</text></view></view></scroll-view></view><view class="f-header m-t"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">猜你喜欢</text><text class="tit2">Guess You Like It</text></view><text class="yticon icon-you"></text></view><view class="guess-section"><block tt:for="{{goodsList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['goodsList','',index]]]]]]]}}" class="guess-item" bindtap="__e"><view class="image-wrapper"><image src="{{item.thumb}}" mode="aspectFill"></image></view><text class="title clamp">{{item.title}}</text><text class="price">{{"￥"+item.price}}</text></view></block></view></view>